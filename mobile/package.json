{
  "name": "canteen-admin-mobile",
  "version": "1.0.0",
  "description": "Canteen Admin Mobile App - Expo",
  "main": "expo-router/build/index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eas": "eas build"
  },
  "dependencies": {
    "expo": "^51.0.0",
    "expo-router": "^3.5.0",
    "expo-constants": "~15.4.0",
    "expo-camera": "~14.1.0",
    "expo-barcode-scanner": "~13.0.0",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-screens": "~3.31.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.10.0",
    "axios": "^1.6.0",
    "zustand": "^4.4.0",
    "react-native-qrcode-svg": "^6.3.0",
    "date-fns": "^2.30.0",
    "expo-clipboard": "~5.0.0",
    "react-native-uuid": "^2.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "expo": {
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ]
  }
}
